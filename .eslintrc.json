{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": "off",
    "require-jsdoc": 0,
    "react/prop-types": 0,
    "quotes": 0,
    "indent": "off",
    "no-unused-vars": "off",
    "comma-dangle": "off",
    "spaced-comment": "off",
    "object-curly-spacing": "off",
    "semi": "off",
    "quote-props": "off",
    "eol-last": "off",
    "padded-blocks": "off",
    "react/no-unescaped-entities": "off",
    "curly": "off",
    "space-before-function-paren": "off",
    "one-var": "off",
    "func-call-spacing": "off",
    "new-cap": "off",
    "linebreak-style": "off",
    "arrow-parens" : "off",
    "no-invalid-this" : "off",
    "prefer-rest-params": "off"
  }
}
